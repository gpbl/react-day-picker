{
  "name": "website",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn build-rdp && TYPEDOC_WATCH=true docusaurus start",
    "build": "docusaurus build",
    "build-rdp": "yarn workspace react-day-picker build",
    "serve": "docusaurus serve",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
