{
  "extends": "@tsconfig/docusaurus/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "esModuleInterop": true,
    "lib": ["es2019", "DOM"],
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": false,
    "paths": {
      "@examples/*": ["./examples/*"],
      "@site/*": ["./*"]
    }
  }
}
