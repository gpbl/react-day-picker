    "rimraf": "^5.0.0",
    "rollup": "^3.20.7",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.5",
    "tslib": "^2.5.0"
