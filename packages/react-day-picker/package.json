{
  "name": "react-day-picker",
  "version": "8.0.0-alpha.0",
  "description": "Flexible Date Picker for React",
  "author": "Giampaolo Bellavite <io@gpbl.dev>",
  "homepage": "http://react-day-picker.js.org",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gpbl/react-day-picker"
  },
  "bugs": {
    "url": "https://github.com/gpbl/react-day-picker/issues"
  },
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "develop": "tsc --watch",
    "lint": "",
    "test": "jest",
    "test:watch": "jest --watchAll --coverage",
    "coverage": "jest --coverage",
    "build-clean": "rm -rf lib && rm -rf dist",
    "build-types": "tsc -p tsconfig.types.json",
    "build-lib": "tsc",
    "build-dist": "cross-env TS_NODE_PROJECT=tsconfig.dist.json webpack",
    "build": "yarn build-clean && yarn build-dist && yarn build-lib && yarn build-types",
    "docs": "yarn docs:markdown && yarn docs:json",
    "docs:markdown": "typedoc --out ../../website/docs/api --theme ./typedoc/themes/default",
    "docs:json": "typedoc --json ../../website/docs/api.json"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/webpack": "^4.41.11",
    "cross-env": "^7.0.2",
    "date-fns": "^2.12.0",
    "debug": "^4.1.1",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.0",
    "ts-node": "^8.8.2",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "webpack": "next",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "date-fns": "^2.12.0"
  }
}
