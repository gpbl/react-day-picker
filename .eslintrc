{
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-console": [
      "error",
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ],
    "no-return-assign": 0,
    "no-restricted-globals": 0,
    "import/named": 2,
    "react/jsx-filename-extension": 0,
    "react/require-default-props": 0
  }
}