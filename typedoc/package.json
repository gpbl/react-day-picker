  "scripts": {
    "build": "pnpm build:latest && pnpm build:next",
    "build-watch": "pnpm build:main --watch",
    "build:latest": "typedoc --options typedoc.latest.mjs",
    "build:next": "typedoc --options typedoc.next.mjs"
  },
  "dependencies": {
    "react-day-picker": "workspace:*",
    "react-day-picker-next": "workspace:*",
    "typedoc": "^0.25.12",
    "typedoc-plugin-frontmatter": "1.0.0-next.1",
    "typedoc-plugin-markdown": "4.0.0-next.54"
  }
}
