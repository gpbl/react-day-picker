    "build": "pnpm build:main && pnpm build:next",
    "build-watch": "pnpm build:next --watch",
    "build:main": "typedoc --options typedoc.main.mjs",
    "build:next": "typedoc --options typedoc.next.mjs"
  },
  "dependencies": {
