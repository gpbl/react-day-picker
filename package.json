{
  "name": "react-day-picker",
  "version": "1.1.0",
  "description": "Customizable date picker and calendar component for React.js",
  "main": "DayPicker.js",
  "scripts": {
    "build": "babel ./src -d ./lib --source-maps && postcss --use autoprefixer src/style.css -d lib/",
    "cover": "istanbul cover -- _mocha --recursive --compilers js:babel/register --reporter spec",
    "test": "mocha --compilers js:babel/register --recursive --reporter spec",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gpbl/react-day-picker.git"
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "calendar",
    "date-picker",
    "datepicker",
    "date",
    "picker",
    "moment",
    "momentjs"
  ],
  "author": "Giampaolo Bellavite <io@gpbl.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gpbl/react-day-picker/issues"
  },
  "homepage": "https://github.com/gpbl/react-day-picker",
  "peerDependencies": {
    "react": "~0.13.x || ~0.14.x"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-eslint": "^4.0.10",
    "classlist-polyfill": "^1.0.1",
    "coveralls": "^2.11.2",
    "eslint": "^1.8.0",
    "eslint-plugin-react": "^3.6.3",
    "exenv": "^1.2.0",
    "fbjs": "^0.4.0",
    "istanbul": "^0.4.0",
    "jsdom": "^7.0.2",
    "mocha": "^2.2.5",
    "moment": "^2.10.3",
    "postcss-cli": "^2.3.2",
    "react": "^0.14.2",
    "react-addons-test-utils": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-tap-event-plugin": "^0.2.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "testdom": "^2.0.0"
  }
}
